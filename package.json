{
  "name": "jarvis-cli",
  "version": "1.0.0",
  "description": "A terminal-based AI assistant powered by Gemini with Spotify and Google Calendar integrations",
  "main": "dist/index.js",
  "bin": {
    "jarvis": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "jarvis": "npm run build && npm start",
    "test": "jest",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "postinstall": "npm run build",
    "package": "npm run build && pkg . --targets node18-win-x64 --out-path release",
    "package:all": "npm run build && pkg . --targets node18-win-x64,node18-macos-x64,node18-macos-arm64,node18-linux-x64 --out-path release"
  },
  "pkg": {
    "assets": [
      "node_modules/**/*"
    ],
    "targets": [
      "node18-win-x64",
      "node18-macos-x64",
      "node18-macos-arm64",
      "node18-linux-x64"
    ],
    "outputPath": "release"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    ".env.example"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "assistant",
    "cli",
    "gemini",
    "spotify",
    "calendar",
    "terminal",
    "jarvis",
    "chatbot",
    "ai-assistant",
    "productivity"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/souptik-taran/jarvis-cli.git"
  },
  "bugs": {
    "url": "https://github.com/SouptikTaran/jarvis-cli/issues"
  },
  "homepage": "https://github.com/SouptikTaran/jarvis-cli#readme",
  "author": "Souptik Taran",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.6.2",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "googleapis": "^129.0.0",
    "ink": "^4.4.1",
    "inquirer": "^8.2.6",
    "node-spotify-api": "^1.1.1",
    "open": "^8.4.2",
    "ora": "^5.4.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "pkg": "^5.8.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
